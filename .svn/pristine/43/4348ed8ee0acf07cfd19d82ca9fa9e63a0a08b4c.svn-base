.UAlert-box {
  background: white;
  box-shadow: 0 12px 20px -10px rgba(81, 64, 49, 0.6);
  position: absolute;
  z-index: 10001;
  left: 50%;
  top: 50%;
  max-width: 600px;
  padding: 10px;
  border: 1px solid #E0E0E0;
}
.UAlert-box .btn-close {
  background-image: url('../images/fancybox/fancybox_sprite.png');
  background-size: 44px 152px;
  position: absolute;
  top: -18px;
  right: -18px;
  width: 36px;
  height: 36px;
  cursor: pointer;
  z-index: 9000;
}
.UAlert-box .UAlert-content {
  min-width: 300px;
  min-height: 80px;
  word-break: break-word;
  max-height: 500px;
  padding: 5px 10px;
  font-size: 16px;
  overflow: auto;
  padding-top: 15px;
}
.UAlert-box .UAlert-box .UAlert-close {
  position: absolute;
  right: 10px;
  top: 10px;
  cursor: pointer;
  font-size: 18px;
}
.UAlert-box .action {
  text-align: center;
}
.UAlert-box .action .btn {
  min-width: 100px;
  font-family: "Microsoft Yahei", sans-serif;
}
/*UAlert end
==============================*/
