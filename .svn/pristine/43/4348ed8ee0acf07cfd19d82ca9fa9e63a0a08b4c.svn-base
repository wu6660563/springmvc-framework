@charset "utf-8";

/* CSS Document */
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, p, blockquote, th, td {
	margin: 0;
	padding: 0;
	box-sizing: border-box;
}
body {
	font-size: 12px;
	color: #434343;
	font-family: "Microsoft Yahei", sans-serif;
	background: #f9f9f9
}
fieldset, img {
	border: 0;
}
ul, p {
	list-style: none;
}
h1, h2, h3, h4, h5, h6 {
	font-size: 100%;
}
em {
	font-style: normal;
}
input, button, select, textarea {
	outline: none;
}
textarea {
	resize: none;
}
a {
	color: #666;
	text-decoration: none;
}
a:hover {
	color: #F34F00;
}
.clearfix:before, .clearfix:after {
	content: "";
	display: table;
}
.clearfix:after {
	clear: both;
}
.clearfix {
	zoom: 1;
}
@font-face {
  font-family: 'fontello';
  src: url('../fonts/fontello.eot?89993121');
  src: url('../fonts/fontello.eot?89993121#iefix') format('embedded-opentype'),
       url('../fonts/fontello.woff2?89993121') format('woff2'),
       url('../fonts/fontello.woff?89993121') format('woff'),
       url('../fonts/fontello.ttf?89993121') format('truetype'),
       url('../fonts/fontello.svg?89993121#fontello') format('svg');
  font-weight: normal;
  font-style: normal;
}
