@font-face {
  font-family: 'fontello';
  src: url('../font/fontello.eot?19869294');
  src: url('../font/fontello.eot?19869294#iefix') format('embedded-opentype'),
       url('../font/fontello.woff2?19869294') format('woff2'),
       url('../font/fontello.woff?19869294') format('woff'),
       url('../font/fontello.ttf?19869294') format('truetype'),
       url('../font/fontello.svg?19869294#fontello') format('svg');
  font-weight: normal;
  font-style: normal;
}
